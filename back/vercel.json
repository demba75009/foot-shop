{
  "version": 2,
  "builds": [
      {
          "src": "*.js",
          "use": "@vercel/node"
      },
      {
        "src": "../front",
        "use": "@vercel/static-build"
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "/"
      },
      {
        "src": "/(.*)",
        "dest": "../front"
      }
  ]
}